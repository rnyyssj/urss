  sendAdminMessage(player, 'Kickear jugadores AFK: `!kickafks`', 6000);

  sendAdminMessage(player, 'B A N E O -- Y -- D E S B A N E O 🚫', 8000);
  sendAdminMessage(player, 'Banear IP: `!banip + IpDeJugador`', 10000);
